@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<KnockAPI.Models.Warehouse>
@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase
@{
    Layout = "Views/Layout/Base.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="text-center">New User</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <form action="/worker/new" method="post" class="form-horizontal">
                <div class="form-group">
                    <div class="col-md-6">
                        <label for="FirstName">Owner First Name</label>
                        <input name="FirstName" value="@Model.FirstName" type="text" class="form-control" placeholder="Owner First Name">
                        <label for="LastName">Owner Last Name</label>
                        <input name="LastName" value="@Model.LastName" type="text" class="form-control" placeholder="Owner Last Name">

                        <label for="TotalCapacity">Total Capacity</label>
                        <input name="TotalCapacity" value="@Model.Capacity"   class="form-control" placeholder="Total Capacity">

                        <label for="Occupied">Total Occupied</label>
                        <input name="Occupied" value="@Model.Occupied" type="text" class="form-control" placeholder="Total Occupied">

                        <label for="StillAvailable">Still Available</label>
                        <input name="StillAvailable" type="text" class="form-control" placeholder="Still Available">

                        <label for="Location">Location</label>
                        <input name="Location" value="@Model.Free" type="text" class="form-control" placeholder="Location">

                        <label for="Contact1">Contact1</label>
                        <input name="Contact1" value="@Model.ContactNo1" type="text" class="form-control" placeholder="Contact1">

                        <label for="Contact2">Contact2</label>
                        <input name="Contact2" value="@Model.ContactNo2" type="text" class="form-control" placeholder="Contact2">

                      


                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-6">
                        <button class="btn btn-primary">Save</button>
                    </div>
                </div>
                
            </form>
        </div>
    </div>
</div>